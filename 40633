        os.system ('bundle install')
        os.system ('bundle update nokogiri')
        os.system ('gem install nokogiri')
        os.system ('gem install nokogiri -- --use-system-libraries')
        os.system ('gem install pkg-config -v "~> 1.1" ')
        os.system ('pkg-config')
        os.system ('bundle update nokogiri')

    elif meta =='3' :
         time.sleep (0.07)
         main ()

    elif meta =='' :
        Dark_meta ()
    else :
        Dark_meta ()



