def Encode ():

    Ds= input ('\033[5;33mEnter Your Txt For \033[5;33mHash >> ')
    Hash1= base64.b85encode (Ds.encode ('ascii'))
    Hash2= base64.b16encode (Hash1)
    Hash3= base64.b32encode (Hash2)
    Hash4= base64.a85encode (Hash3)
    Hash5= base64.b64encode (Hash4)

    time.sleep (0.1)
    print ('    ')
    print ('\033[5;37m-------------------------------->>')
    print ('\033[5;33mDone...! >>','\033[5;32m[\033[5;31m',Hash5,'\033[5;32m]')
    print ('\033[5;37m-------------------------------->>')
    print ('      ')
    input ('=>> ')
    os.system ('clear')
    DShash()
#################################

def Decode ():

    Ds=input ('Enter Your Hash For Decode >> ')
    DHash1= base64.b64decode (Ds)
    DHash2= base64.a85decode (DHash1)
    DHash3= base64.b32decode (DHash2)
    DHash4= base64.b16decode (DHash3)
    DHash5= base64.b85decode (DHash4)

    time.sleep (0.1)
    print ('    ')
    print ('\033[5;37m------------------------------>>')
    print ('\033[5;33mDone...! >>','\033[5;32m[\033[5;31m',DHash5,'\033[5;32m]')
    print ('\033[5;37m------------------------------>>')
    print ('      ')
    input ('=>> ')
    os.system ('clear')
    DShash ()

