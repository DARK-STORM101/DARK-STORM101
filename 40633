    time.sleep  (0.1)
    print ('\033[5;31m| | | | \___  \  | |_| |   / ___ \   \___  \   |  _  | ')
    time.sleep  (0.1)
    print ('\033[5;32m| |_| |  ___| |  | | | |  / /   \ \   ___| |   | | | | ')
    time.sleep  (0.1)
    print ('\033[5;33m|____/  /_____/  |_| |_| /_/     \_\ /_____/   |_| |_| ')
    time.sleep  (0.1)
    print ('	')
    print ('\033[5;32m- \033[5;33mDark_Storm \033[5;32m-')
    time.sleep  (0.1)
