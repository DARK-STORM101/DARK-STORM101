####################################
def main() :

    Ds= input ('\033[1;37mDS \033[1;32m>>\033[1;33m ')
    if Ds=='help' :
        time.sleep (0.09)
        help()

    elif Ds=='Help' :
        time.sleep (0.09)
        help()
    elif Ds=='':
        main()
    elif Ds =='termux-tool' :
        Termux_Tool ()
    elif Ds=='Termux-Tool':
        Termux_Tool ()
    elif Ds=='metasploit':
        Dark_meta ()
    elif Ds=='Metasploit':
        Dark_meta()
    elif Ds=='DARK STORM_HASH':
        DShash ()
    elif Ds=='dark storm_hash':
        DShash ()
    elif Ds=='themes-tool':
        themestool()
        themes()
    elif Ds=='Encrypt Files':
        banner_Tool ()
        home_Tool ()
    elif Ds=='encrypt files':
        banner_Tool ()
        home_Tool ()
    elif Ds=='clear':
        os.system ('clear')
        main ()
    elif Ds=='banner':
        banner_Dark()
        main ()
    elif Ds=='Fonts-Fotoshop':
        fonts ()
    elif Ds=='banner1':
        banner()
        main ()
    elif Ds=='banner2':
         banner2 ()
         main ()
    elif Ds=='info termux-tool':
        time.sleep (0.09)
        Infotermux()
    elif Ds=='info metasploit':
        time.sleep (0.09)
        Infometa()
    elif Ds=='info darkstorm hash':
        time.sleep (0.09)
        darkstormhash()
    elif Ds=='info fontfotoshop':
        time.sleep (0.09)
        fontfotoshop ()
    elif Ds=='info encryptfiles':
        time.sleep (0.09)
        encryptfiles()
    elif Ds=='info themestool':
        time.sleep (0.09)
        infothemestool ()
    elif Ds=='exit':
        time.sleep (2)
    else:
        print ('Error....!')
        main()
