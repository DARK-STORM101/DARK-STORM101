    print ('    ')
    time.sleep  (0.1)
    print ('\033[5;31m╭──────────────────────────────────────────────────────────────╮ ')
    time.sleep  (0.1)
    print ('\033[5;31m│\033[5;32m>>>>>>>>>>>>>>>>>>>>>\033[5;32m[\033[5;33mDark_Storm\033[5;32m]\033[5;31m#\033[5;32m[\033[5;33mHASH\033[5;31m]\033[5;32m<<<<<<<<<<<<<<<<<<<<<<\033[5;31m│ ')
    time.sleep  (0.1)
    print ('\033[5;31m│──────────────────────────────────────────────────────────────│ ')
    time.sleep  (0.1)
