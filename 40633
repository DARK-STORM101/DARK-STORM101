def passw():
    Ds2=input ('\033[5;32mEnter \033[5;31mYour \033[5;36mPassword \033[5;32m:\033[5;33m ')
    if Ds2=='dark101' :
        print ('')
        print ('\033[5;32m{~}\033[5;31m Please Wait\033[5;33m....')
        time.sleep (1)
        banner ()
    elif Ds2=='Dark Storm' :
        print ('')
        print ('\033[5;32m{~}\033[5;31m Please Wait\033[5;33m....')
        time.sleep (1)
        banner ()
    else:
        print ('Error On Password = '+Ds2)
        time.sleep (10)
        username ()
